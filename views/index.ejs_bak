      <div class="row">
        <div class="col-lg-3">
          <a href="http://platform.datap.ai/#/pipeline">
            <div class="feature1 feature-item" alt="Generic placeholder image"></div>
            <h2>Data Integration(DI)</h2>
          </a>
        </div><!-- /.col-lg-4 -->
        <div class="col-lg-3">
