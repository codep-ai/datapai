<%- include('_header') %>

<body>
  <header>
    <%- include('_menu') %>
  </header>

  <main role="main">
    <section class="jumbotron text-center" id="hero">
      <div class="container">
        <div class="row activate-wrapper">
        <div class="col-lg-6">
          <div>
            <h1 class="intro-header__title text-white">
              Set up Reliable Data Pipelines in Minutes
            </h1>
          </div>

          <div>
            <p class="lead-lg intro-header__lead text-black-60">
              DataPAI(Data+AI) is a no-code and highly intuitive data platform.
              It loads data from many source  to your warehouse in real-time
              for faster analytics.
            </p>
          </div>
          <div class="feature-section-footer text-center" style="margin-top: 15px;">
            <a href="http://platform.datap.ai/#/" data-track-click="Explore DataPAI Data Pipeline Click"
              data-track-category="Homepage - Reliable data pipelines Section" class="btn btn-primary btn-sm">
              Explore DataPai Data Pipeline
            </a>
          </div>
          <div class="mt-8">
            <form class="free-trial-form input-group-stacked-btn-wrapper invalid" novalidate="" autocomplete="off"
              data-section="Top Banner" data-product="" data-hs-cf-bound="true" data-factors-bind="true">
              <input type="hidden" name="csrfmiddlewaretoken"
                value="iqh2HNC1mZZ4cb0lxkV6NJclw2y5pmwy2DfyV5c51rPHCtrYKi1qvLVK7WXyTUOh">
              <div class="input-group">
                <input name="email" type="email" class="form-control form-control-sm input-tooltip invalid"
                  placeholder="name@company.com" aria-label="name@company.com" required="">
                <div class="input-group-append">
                  <span class="DataPAI-web-icon DataPAI-error-circle input-group-glyph form-error-glyph"
                    data-placement="bottom" data-form-error-tooltip="email"
                    data-default-error-title="Please enter your business email : <span class='font-weight-semi-bold'>name@company.com</span>">
                  </span>
                </div>
              </div>
              <button type="submit" class="btn btn-primary btn-sm stacked-btn trial-button" data-factors-bind="true">
                <span class="btn-main-body">
                  Get Started for Free
                </span>
                <img class="btn-loading-indicator"
                  src="https://res.cloudinary.com/DataPAI/image/upload/v1650395517/DataPAI-website/loaders/btn-secondary-loading_dsw2wj.svg">
              </button>
            </form>
          </div>

          <span class="watch-link">
            <span class="badge home-intro-video" data-track-click="Watch Now Video" data-track-category="Top Banner">
              <span class="DataPAI-play DataPAI-web-icon mr-1"></span> Watch Now
            </span>
            <span class="link-text d-inline-block">Fastest and easiest way to bring any data into your data
              warehouse</span>
          </span>        
        </div>
        <div class="col-lg-6 d-flex align-items-center">
          <img class="featurette-image img-fluid mx-auto" src="/images/datapai-logo.png" alt="datapai logo" style="width:400px;height:400px">
        </div>
      </div>       
      </div>     
    </section>
    </div>


